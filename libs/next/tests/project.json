{
  "name": "next-tests",
  "$schema": "../../../node_modules/nx/schemas/project-schema.json",
  "sourceRoot": "libs/next/tests/src",
  "projectType": "library",
  "tags": [],
  "targets": {
    "lint": {
      "executor": "@nx/linter:eslint",
      "outputs": ["{options.outputFile}"],
      "options": {
        "lintFilePatterns": ["libs/next/tests/**/*.{ts,tsx,js,jsx}"]
      }
    },
    "typecheck": {
      "executor": "nx:run-commands",
      "options": {
        "commands": ["tsc -p libs/next/tests/tsconfig.json"]
      }
    },
    "test": {
      "executor": "@nx/vite:test",
      "outputs": [],
      "options": {
        "passWithNoTests": false,
        "reportsDirectory": "../../../coverage/libs/next/tests",
        "config": "{workspaceRoot}/vitest.config.mts"
      }
    }
  }
}
