    "variant": {
      "type": "simple",
      "question": "What is the difference between UTC and GMT?",
      "options": [
        {
          "id": "AT4APS0EQU",
          "text": "UTC is slightly more accurate than GMT, but the difference between the two is so small that it has no importance in everyday navigation of aircraft.",
          "correct": true,
          "why": ""
        },
        {
          "id": "ARZ2AC9SXB",
          "text": "All answers above are correct.",
          "correct": false,
          "why": ""
        },
        {
          "id": "A543YKOHYQ",
          "text": "GMT is valid only at the Greenwich meridian and is of no use at other longitudes.",
          "correct": false,
          "why": ""
        },
        {
          "id": "AIW7A7AUI4",
          "text": "UTC is adjusted abruptly at announced time and is hence not practical to use.",
          "correct": false,
          "why": ""
        }
      ]
    },
