        },
        {
          "id": "A40KAK33PB",
          "text": "The duration of the apparent solar day is constant throughout a year due to the constant rotational speed of the Earth around its axis.",
          "correct": false,
          "why": ""
        },
        {
          "id": "ARW6O492WD",
          "text": "The apparent solar day is the period between two successive transits of the mean Sun through the same meridian.",
          "correct": false,
          "why": ""
        },
        {
          "id": "AB5SM6TLR5",
          "text": "The duration of the apparent solar day is constant throughout a year due to the constant velocity of the Earth in its orbit around the Sun.",
          "correct": false,
          "why": ""
        }
      ]
