    "variant": {
      "type": "simple",
      "question": "When is Radiation fog likely to form?",
      "options": [
        {
          "id": "OQVJNMR3C",
          "text": "When the Earth radiates heat at night into the air above it, warming it above its dew point and so condensing its water vapour into fog droplets.",
          "correct": false,
          "why": ""
        },
        {
          "id": "OIWGCI5I0",
          "text": "When the Sun's radiation warms the air in contact with the Earth's surface above its dew point and the evaporated water droplets are supported by wind blowing up a slope.",
          "correct": false,
          "why": ""
        },
        {
          "id": "ODMPRTDKQ",
          "text": "When the Earth radiates heat into space; air in contact with Earth's surface is cooled below its dew point and the condensed droplets are supported by mechanical turbulence.",
          "correct": true,
          "why": ""
        },
        {
          "id": "ODWW85XY3",
          "text": "When the Sun's radiation evaporates water from wet ground. The saturated air is cooled below its dew point as it flows over cold ground in winter, and the condensed droplets are supported by the wind.",
          "correct": false,
          "why": ""
        }
      ]
    }
