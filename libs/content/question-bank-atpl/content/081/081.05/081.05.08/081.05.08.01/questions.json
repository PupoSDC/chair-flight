    "variant": {
      "type": "simple",
      "question": "What is the fundamental difference between a trim tab and a servo tab?",
      "options": [
        {
          "id": "A7VH77LQGA",
          "text": "The main purpose of a trim tab is to reduce stick force to zero in steady flight, a servo tab only reduces stick force.",
          "correct": true,
          "why": ""
        },
        {
          "id": "AUZ7VCDV4O",
          "text": "A servo tab affects the stick force stability, whereas a trim tab does not.",
          "correct": false,
          "why": ""
        },
        {
          "id": "AKPQ5KX4NY",
          "text": "The functioning of a trim tab is based on aerodynamic balancing, whereas a servo tab is usually adjusted via a screwjack.",
          "correct": false,
          "why": ""
        },
        {
          "id": "AOLZC7PPDT",
          "text": "A trim tab is automatically adjusted when its particular control surface moves, whereas a servo tab is moved independently of its particular control surface.",
          "correct": false,
          "why": ""
        }
      ]
    },
