{
  "name": "next-app",
  "$schema": "../../node_modules/nx/schemas/project-schema.json",
  "sourceRoot": "apps/next-app",
  "projectType": "application",
  "targets": {
    "dev": {
      "executor": "@nx/next:server",
      "dependsOn": ["^deploy", "^compile", "^generate"],
      "options": {
        "port": 4200,
        "buildTarget": "next-app:build",
        "dev": true
      }
    },
    "build": {
      "executor": "@nx/next:build",
      "outputs": ["{options.outputPath}"],
      "dependsOn": ["^deploy", "^compile", "^generate", "docs:build"],
      "options": {
        "root": "apps/next-app",
        "outputPath": ".dist/apps/next-app"
      }
    },
    "serve": {
      "executor": "@nx/next:server",
      "options": {
        "buildTarget": "next-app:build",
        "dev": false
      }
    },
    "test": {
      "executor": "@nx/vite:test",
      "options": {
        "passWithNoTests": true,
        "reportsDirectory": "../../coverage/apps/next-app",
        "config": "{projectRoot}/vitest.config.ts"
      }
    },
    "lint": {
      "executor": "@nx/linter:eslint",
      "outputs": ["{options.outputFile}"],
      "options": {
        "lintFilePatterns": ["apps/next-app/pages/**/*.{ts,tsx,js,jsx}"]
      }
    },
    "typecheck": {
      "executor": "nx:run-commands",
      "options": {
        "commands": [
          "tsc -p apps/next-app/tsconfig.app.json --noEmit",
          "tsc -p apps/next-app/tsconfig.spec.json --noEmit"
        ]
      }
    }
  },
  "implicitDependencies": ["apps/docs"],
  "tags": []
}
